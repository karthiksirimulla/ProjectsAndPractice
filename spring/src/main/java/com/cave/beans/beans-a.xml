<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="fruitBasket" class="com.cave.part2.FruitBasket">
		<constructor-arg name="bname" value="myBasket"></constructor-arg>
		<constructor-arg>
			<list>
				<value>Orange</value>
				<value>Apple</value>
				<value>Banana</value>
				<value>Kiwi</value>
				<value>Pear</value>
				<value>Orange</value>
				<value>Orange</value>
				<value>Orange</value>
				<value>Orange</value>
			</list>
		</constructor-arg>
	</bean>
</beans>
